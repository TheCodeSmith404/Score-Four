<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayerIcon">
        <attr name="playerText" format="string" />
    </declare-styleable>
    <declare-styleable name="WaitingRoomItem">
        <attr name="playerName" format="string"/>
    </declare-styleable>
    <declare-styleable name="CurvedTextView">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="arcHeight" format="dimension" />
        <attr name="textStyle">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
        <attr name="arcHeightRatio" format="dimension"/>
        <attr name="centerX" format="float" />
        <attr name="centerY" format="float" />
<!--        <attr name="fontFamily" format="dimension" />-->
    </declare-styleable>
</resources>